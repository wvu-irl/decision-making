{
    "stitch": [
        {
            "combo": [
                "algs",
                "envs"
            ]
        }
    ],
    "envs": [
        {
            "env": "irl_gym/GridWorld-v0",
            "max_time": 100,
            "params": {
                "state": {
                    "pose": [20, 20 ]
                },
                "goal": [10, 10],
                "r_radius": 5,
                "render": "none",
                "cell_size": 50,
                "save_frames": false,
                "log_level": "WARNING",
                "p": 0.1,
                "dimensions": [
                    50,
                    50
                ],
                "r_range": [
                    -0.01,
                    1
                ]
            }
        }
    ],
    "algs": [
        {
            "alg": "aogs",
            "params": {
                "max_iter": 6000.0,
                "gamma": 0.95,
                "max_graph_size": 50000.0,
                "rng_seed": 45,
                "model_accuracy": {
                    "epsilon": 0.2,
                    "delta": 0.1
                },
                "action_selection": {
                    "function": "ambiguity_aware",
                    "params": {
                        "alpha": 0
                    }
                }
            },
            "search": {
                "max_samples": 500,
                "horizon": 50,
                "timeout": 5,
                "reinit": true
            }
        }
    ]
}